{
  "manifest_version": 3,
  "name": "Minimal Blocker",
  "version": "0.2.0",
  "description": "Blocks websites listed in the extension management page.",
  "permissions": [
    "storage",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Block Websites"
  },
  "options_page": "manager.html"
}
